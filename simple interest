#include<stdio.h>
int main()
{
	float si,p,n;
	int r,age;
	printf("enter principle amount");
	scanf("%f",&p);
	printf("enter number of years");
	scanf("%f",&n);
	if(p>0&&n>0&&n<30)
	{
		printf("rnter age");
		scanf("%d",&age);
		if(age<=65)
		r=12;
		else
		r=10;
		si=p*n*r/100;
		printf("enter simple interest is %f",si);
	}
	else
	printf("invalid");
}
